<?xml version="1.0" encoding="UTF-8"?>
<form>
    <tnotebook id="0">
        <class>TNotebook</class>
        <name>notebook1</name>
        <x>10</x>
        <y>10</y>
        <width>430</width>
        <height>153</height>
        <pages>
            <tpanel id="1" tab="Check out">
                <tlabel id="0">
                    <class>TLabel</class>
                    <name>member_label</name>
                    <x>8</x>
                    <y>20</y>
                    <color>#000000</color>
                    <size>10</size>
                    <style></style>
                    <value>Member</value>
                </tlabel>
                <tseekbutton id="1">
                    <class>TSeekButton</class>
                    <name>member_id</name>
                    <database>library</database>
                    <model>Member</model>
                    <display>name</display>
                    <receiver>member_name</receiver>
                    <x>80</x>
                    <y>10</y>
                    <width>88</width>
                    <editable>1</editable>
                </tseekbutton>
                <tentry id="2">
                    <class>TEntry</class>
                    <name>member_name</name>
                    <x>205</x>
                    <y>10</y>
                    <value></value>
                    <mask></mask>
                    <width>190</width>
                    <editable>0</editable>
                </tentry>
                <tbutton id="3">
                    <class>TButton</class>
                    <name>add_button</name>
                    <x>15</x>
                    <y>80</y>
                    <value>Adicionar</value>
                    <icon>ico_add.png</icon>
                    <action>onAdd</action>
                    <template>save.php</template>
                </tbutton>
                <tbutton id="4">
                    <class>TButton</class>
                    <name>apply_button</name>
                    <x>115</x>
                    <y>80</y>
                    <value>Aplicar</value>
                    <icon>ico_ok.png</icon>
                    <action>onSave</action>
                    <template>save.php</template>
                </tbutton>
                <tseekbutton id="5">
                    <class>TSeekButton</class>
                    <name>barcode_input</name>
                    <database>0</database>
                    <model>0</model>
                    <display>0</display>
                    <receiver></receiver>
                    <x>80</x>
                    <y>45</y>
                    <width>88</width>
                    <editable>1</editable>
                </tseekbutton>
                <tentry id="6">
                    <class>TEntry</class>
                    <name>book_title_input</name>
                    <x>205</x>
                    <y>45</y>
                    <value></value>
                    <mask></mask>
                    <width>190</width>
                    <editable>0</editable>
                </tentry>
                <tlabel id="7">
                    <class>TLabel</class>
                    <name>barcode_label</name>
                    <x>8</x>
                    <y>50</y>
                    <color>#000000</color>
                    <size>10</size>
                    <style></style>
                    <value>Barcode</value>
                </tlabel>
            </tpanel>
        </pages>
    </tnotebook>
    <tframe id="1">
        <class>TFrame</class>
        <name>frame2</name>
        <title>Items</title>
        <x>10</x>
        <y>170</y>
        <width>430</width>
        <height>160</height>
        <child>
            <tpanel>
                <tdatagrid id="0">
                    <class>TDataGrid</class>
                    <name>items</name>
                    <x>5</x>
                    <y>0</y>
                    <loader>onReload</loader>
                    <template>reload.php</template>
                    <width>370</width>
                    <height>132</height>
                    <columns>
                        <tdatagridcolumn id="1">
                            <name>barcode</name>
                            <align>left</align>
                            <label>Item ID</label>
                            <width>103</width>
                        </tdatagridcolumn>
                        <tdatagridcolumn id="2">
                            <name>book_title</name>
                            <align>left</align>
                            <label>Book title</label>
                            <width>233</width>
                        </tdatagridcolumn>
                    </columns>
                    <actions>
                        <tdatagridaction id="1">
                            <method>onDelete</method>
                            <image>ico_delete.png</image>
                            <field>barcode</field>
                            <label>Delete</label>
                            <template>delete.php</template>
                        </tdatagridaction>
                    </actions>
                    <pagenavigator>no</pagenavigator>
                </tdatagrid>
            </tpanel>
        </child>
    </tframe>
</form>
